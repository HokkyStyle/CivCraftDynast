  <patternset id="excluded.from.compilation.civilizationcraftidea">
    <patternset refid="excluded.from.module.civilizationcraftidea"/>
  </patternset>
  
  <path id="civilizationcraftidea.module.sourcepath">
    <dirset dir="${module.civilizationcraftidea.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.civilizationcraftidea" depends="compile.module.civilizationcraftidea.production,compile.module.civilizationcraftidea.tests" description="Compile module CivilizationCraftIDEA"/>
  
  <target name="compile.module.civilizationcraftidea.production" depends="register.custom.compilers" description="Compile module CivilizationCraftIDEA; production classes">
    <mkdir dir="${civilizationcraftidea.output.dir}"/>
    <javac2 destdir="${civilizationcraftidea.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.civilizationcraftidea}/javac">
      <compilerarg line="${compiler.args.civilizationcraftidea}"/>
      <bootclasspath refid="civilizationcraftidea.module.bootclasspath"/>
      <classpath refid="civilizationcraftidea.module.production.classpath"/>
      <src refid="civilizationcraftidea.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.civilizationcraftidea"/>
    </javac2>
    
    <copy todir="${civilizationcraftidea.output.dir}">
      <fileset dir="${module.civilizationcraftidea.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.civilizationcraftidea.tests" depends="register.custom.compilers,compile.module.civilizationcraftidea.production" description="compile module CivilizationCraftIDEA; test classes" unless="skip.tests"/>
  
  <target name="clean.module.civilizationcraftidea" description="cleanup module">
    <delete dir="${civilizationcraftidea.output.dir}"/>
    <delete dir="${civilizationcraftidea.testoutput.dir}"/>
  </target>
